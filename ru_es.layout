// Based on:
// — Modified for a real Spanish keyboard by Jon Tombs.
// — typographic keyboard by Sergei Stolyarov <sergei@regolit.com>
//    — Initial contribution by Alexey Ten <alexeyten+deb@gmail.com>
//    — Based on the layout proposed by Ilya Birman,
//      http://ilyabirman.ru/typography-layout/

partial hidden alphanumeric_keys
xkb_symbols "mudasobwa" {

// 1st keyboard row
    key <AE01>  { [      grave,          1,  onesuperior,        U2135 ] }; // "1" → ` 1 ¹ ℵ
    key <AE05>  { [    percent,          5,         plus,        U2030 ] }; // "5" → % 5 + ‰
    key <AE06>  { [asciicircum,          6,     asterisk,      notsign ] }; // "6" → ^ 6 * ¬
    key <AE07>  { [  ampersand,          7,        slash,        U0338 ] }; // "7" → & 7 / /
    key <AE08>  { [  parenleft,          8,        U221E,        U03BC ] }; // "8" → ( 8 ∞ μ
    key <AE09>  { [ parenright,          9,        U21D2,        U21D0 ] }; // "9" → ) 9 ⇒ ⇐
    key <AE10>  { [      equal,          0,        U21D3,        U21D1 ] }; // "0" → = 0 ⇓ ⇑
    key <AE11>  { [ apostrophe,   quotedbl,       emdash,       endash ] }; // "-" → ' " — –
    key <AE12>  { [     exclam,   question,   exclamdown, questiondown ] }; // "=" → ! ? ¡ ¿

// 2nd keyboard row
    key <AD01> { [    NoSymbol,   NoSymbol,        U231A,        U231B ] }; // "q" → ̲ ̲ ⌚ ⌛ // TODO
    key <AD02> { [    NoSymbol,   NoSymbol,        U2200,        U2208 ] }; // "w" → ̲ ̲ ∀ ∈ // TODO
    key <AD03> { [    NoSymbol,   NoSymbol,        U2203,        U2204 ] }; // "e" → ̲ ̲ ∃ ∄ // TODO
    key <AD04> { [    NoSymbol,   NoSymbol,   registered,        U221A ] }; // "r" → ̲ ̲ ® √ // TODO
    key <AD05> { [    NoSymbol,   NoSymbol,    trademark,        U2709 ] }; // "t" → ̲ ̲ ™ ✉
    key <AD06> { [    NoSymbol,   NoSymbol,     EuroSign,          yen ] }; // "y" → ̲ ̲ € ¥
    key <AD07> { [    NoSymbol,   NoSymbol,        U25B6,        U25B7 ] }; // "u" → ̲ ̲ ▶ ▷
    key <AD08> { [    NoSymbol,   NoSymbol,        U2022,        U25E6 ] }; // "i" → ̲ ̲ • ◦
    key <AD09> { [    NoSymbol,   NoSymbol,        U25C9,        U25CE ] }; // "o" → ̲ ̲ ◉ ◎
    key <AD10> { [    NoSymbol,   NoSymbol,        acute,  doubleacute ] }; // "p" → ̲ ̲ ´ ˝

// 3rd keyboard row
    key <AC01> { [    NoSymbol,   NoSymbol,        U0301,        U0300 ] }; // "a" → ̲ ̲ ́  ̀
    key <AC02> { [    NoSymbol,   NoSymbol,      section,        U23CE ] }; // "s" → ̲ ̲ § ⏎
    key <AC03> { [    NoSymbol,   NoSymbol,       degree,        UFFFD ] }; // "d" → ̲ ̲ ° �
    key <AC04> { [    NoSymbol,   NoSymbol,        U261B,        U261E ] }; // "f" → ̲ ̲ ☛ ☞
    key <AC05> { [    NoSymbol,   NoSymbol,        U2690,        U2691 ] }; // "g" → ̲ ̲ ⚐ ⚑
    key <AC06> { [    NoSymbol,   NoSymbol,        U2764,        U03C0 ] }; // "h" → ̲ ̲ ❤ π
    key <AC07> { [    NoSymbol,   NoSymbol, doublelowquotemark, singlelowquotemark] }; // "j" → ̲ ̲ „ ‚
    key <AC08> { [    NoSymbol,   NoSymbol, leftdoublequotemark, leftsinglequotemark] }; // "k" → ̲ ̲ “ ‘
    key <AC09> { [    NoSymbol,   NoSymbol, rightdoublequotemark, rightsinglequotemark] }; // "l" → ̲ ̲ ” ’
    key <AC10> { [    NoSymbol,   NoSymbol,     ccedilla,     Ccedilla ] }; // "ñ" → ̲ ̲ ç Ç

// 4th keyboard row
    key <AB01> { [    NoSymbol,   NoSymbol,        colon,        U22C5 ] }; // "z" → ̲ ̲ « <
    key <AB02> { [    NoSymbol,   NoSymbol,    semicolon,     multiply ] }; // "x" → ̲ ̲ × ⋅
    key <AB03> { [    NoSymbol,   NoSymbol,    copyright,         cent ] }; // "c" → ̲ ̲ © ¢
    key <AB04> { [    NoSymbol,   NoSymbol,        U2606,        U2605 ] }; // "v" → ̲ ̲ ☆ ★
    key <AB05> { [    NoSymbol,   NoSymbol,        U266F,        U235F ] }; // "b" → ̲ ̲   ⍟
    key <AB06> { [    NoSymbol,   NoSymbol,        U2622,        U267B ] }; // "n" → ̲ ̲ ☢ ♻
    key <AB07> { [    NoSymbol,   NoSymbol,        U270E,        U270D ] }; // "m" → ̲ ̲ ✎ ✍
    key <AB08> { [    NoSymbol,   NoSymbol, guillemotleft,       U2208 ] }; // "," → ̲ ̲ « ⊂
    key <AB09> { [    NoSymbol,   NoSymbol, guillemotright,      U2209 ] }; // "." → ̲ ̲ » ⊃

// 5th keyboard row
    key <SPCE> { [       space,      space, nobreakspace,        U200B ] }; // " " → spaces

    key.type[group1]="FOUR_LEVEL";
};

partial hidden alphanumeric_keys
xkb_symbols "mudasobwa_ru" {
    key <TLDE>  { [   NoSymbol,   NoSymbol,   asciitilde,          bar ] }; // "~" → ~ | ≡ ≈

    key <AE02>  { [      colon,          2,           at,      onehalf ] }; // "2" → @ 2 † ½
    key <AE03>  { [      minus,          3,   underscore,        U266B ] }; // "3" → - 3 _ ♫
    key <AE04>  { [  semicolon,          4,        U2713,        U2717 ] }; // "4" → $ 4 ✓ ✗
    key <AB10>  { [     period,      comma,     ellipsis,   numerosign ] };
    key <AC11>  { [   NoSymbol,   NoSymbol,        U0304,        U0307 ] };
    key <AD11>  { [   NoSymbol,   NoSymbol,  bracketleft, bracketright ] };
    key <AD12>  { [   NoSymbol,   NoSymbol,    braceleft,   braceright ] };
    key <LSGT>  { [      minus,      U2239,        U03BB,        U039B ] };
    key <AA02>  { [      U2237,      U2239,        U223B,        U262B ] };
    key <BKSL>  { [     emdash,     endash,        slash,    bachslash ] }; // — – / \
};

partial hidden alphanumeric_keys
  xkb_symbols "mudasobwa_es" {
    key <TLDE>  { [ asciitilde,        bar,        U2261,     approxeq ] }; // "~" → ~ | ≡ ≈

    key <AE02>  { [         at,          2,  twosuperior,      onehalf ] }; // "2" → @ 2 $ ½
    key <AE03>  { [ numbersign,          3,        U2021,        U266B ] }; // "3" → # 3 ‡ ♫
    key <AE04>  { [     dollar,          4,        U2713,        U2717 ] }; // "4" → $ 4 ✓ ✗
    key <AB10>  { [      minus, underscore,        U0336,        U0332 ] };
    key <AC11>  { [  braceleft, braceright,        U0308,        U0303 ] };
    key <AD11>  { [ bracketleft, bracketright,  ellipsis,          euro] };
    key <AD12>  { [       less,    greater,     notequal,    plusminus ] };
    key <LSGT>  { [      U03BB,      U039B,        U0302,        U0306 ] };
    key <BKSL>  { [      slash,  backslash,        U0302,       endash ] }; // "" → ^ — ̂ –
};

